{
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "flowtype/define-flow-type": 1,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-unknown-property": 1,
    "react/react-in-jsx-scope": 1
  },
  "env": {
    "browser": true,
    "node": true,
  },
  "globals": {
    "Promise": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "flowtype",
    "react"
  ]
}
